public class Prime 
{
	public static void main(String[] args) 
	{
     f(3);
	}
	static void f(int n){
		int count=0;
		for(int i=2;i*i<=n;i++){
			if(n%i==0){
				count++;
			}
			if((n/i)!=i){
				count++;
			}
		}
		if(count==2){
			System.out.println(" Number is  prime");
		}
		else{
			System.out.println("Number is not prime");
		}
	}
}

